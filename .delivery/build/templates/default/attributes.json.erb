{
  "qa-chef-server-cluster": {
    "chef-server": {
      "flavor": "<%= node['chef_server_flavor'] %>",
      <% if node['chef_server_test_url_override'] %>
        "url": "<%= node['chef_server_test_url_override'] %>"
      <% else %>
        "version": "<%= node['chef_server_upgrade_from_version'] || node['chef_server_test_version'] %>",
        "channel": "<%= node['chef_server_upgrade_from_channel'] || node['chef_server_test_channel'] %>"
      <% end %>
    },
    "aws": {
      "machine_options": {
        "aws_tags": {
          "delivery_stage": "<%= node['delivery']['change']['stage'] %>",
          "X-Project": "qa-chef-server-cluster-delivery-builder"
        },
        "ssh_username": "ubuntu",
        "bootstrap_options": {
          "image_id": "<%= node['ami']['ubuntu-12.04'] %>",
          "instance_type": "<%= node['chef_server_instance_size'] %>"
        }
      }
    }
  }
}
